{
  "name": "wedding-site",
  "description": "Joe and Jasmine's wedding site",
  "scripts": {
    "build": "webpack --config webpack.prod.js && cp src/_redirects dist",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "start": "npm run dev",
    "test": "npm run test:types && npm run test:lint && npm run test:prettier",
    "test:lint": "eslint . --max-warnings=0 --ext .js,.jsx,.ts,.tsx",
    "test:prettier": "prettier --list-different .",
    "test:types": "tsc",
    "storybook": "start-storybook -p 6006"
  },
  "author": "Joe Brady",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/react": "6.1.21",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "compression-webpack-plugin": "6.0.2",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-loader": "1.3.1",
    "html-webpack-plugin": "4.5.0",
    "prettier": "2.2.1",
    "typescript": "4.2.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.4"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Joe-Brady/wedding-site.git"
  }
}
